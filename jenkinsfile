pipeline {
    agent any
    stages {
        stage ('GIT') {
            steps {
                echo "Getting Project from Git";
                git "https://github.com/achrafxor/devopsesprit"
            }
        }
        stage ('MVN CLEAN') {
            steps {
                echo "Maven Clean";
                bat 'mvn clean';
            }
        }
        stage ('MVN TEST') {s
            steps {
                echo "Maven Test JUnit";
                bat 'mvn test';
            }
        }
    }
}